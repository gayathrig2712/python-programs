nterms=int(input("Enter how many terms you want: "))
n1,n2=0,1
count=0
if nterms<0:
    print("Please enter a positive number")
elif nterms==0:
    print("Fibonocci series is :",n1)
else:
    print("The fibonocci series upto the given range is: ")
    while count<nterms:
        print(n1)
        nth=n1+n2
        n1=n2
        n2=nth
        count=count+1
        

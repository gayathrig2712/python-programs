way-1
def biggest(a,b):
    if a>b:
        print("a is biggest")
    else:
        print("b is biggest")
a=int(input("Enter a number: "))
b=int(input("Enter another number: "))
biggest(a,b)
way-2
def biggest(a,b):
    if a>b:
        return "a is big"
    else:
        return "b is big"
a=int(input("Enter a number: "))
b=int(input("Enter another number: "))
print(biggest(a,b))
way-3
def biggest(a,b):
    if a>b:
        return "True"
    else:
        return "False"
a=int(input("Enter a number: "))
b=int(input("Enter another number: "))
if(biggest(a,b)):
    print("a is big")
else:
    print("b is big")
